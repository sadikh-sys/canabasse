{
  "name": "prp-music-platform",
  "version": "1.0.0",
  "description": "Secure music streaming platform with limited listening access",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "backend:dev": "cd backend && npm run dev",
    "backend:build": "cd backend && npm run build",
    "backend:start": "cd backend && npm start",
    "backend:test": "cd backend && npm test",
    "backend:db:push": "cd backend && npm run db:push",
    "backend:db:init": "cd backend && npm run db:init",
    "backend:db:studio": "cd backend && npm run db:studio",
    "dev:all": "concurrently \"npm run backend:dev\" \"npm run dev\"",
    "build:all": "npm run backend:build && npm run build",
    "start:all": "concurrently \"npm run backend:start\" \"npm run start\"",
    "test:all": "npm run backend:test",
    "setup": "npm install && cd backend && npm install",
    "clean": "rm -rf .next node_modules backend/node_modules backend/dist backend/coverage"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@supabase/supabase-js": "^2.76.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "axios": "^1.6.2",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "react-player": "^2.13.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.0.4",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "keywords": [
    "music",
    "streaming",
    "secure",
    "payment",
    "nextjs",
    "typescript"
  ],
  "author": "PRP Music Platform",
  "license": "MIT"
}
